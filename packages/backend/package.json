{
  "name": "@nfl-predictor/backend",
  "version": "1.0.0",
  "description": "NFL Predictor Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "node src/db/wait-for-postgres.js && npm run reset-failed-migration; npm run migrate && node src/server.js",
    "wait-for-db": "node src/db/wait-for-postgres.js",
    "reset-failed-migration": "node src/db/reset-failed-migration.js || true",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "lint": "eslint src/",
    "migrate": "node src/db/migrate.js",
    "seed": "node src/db/seed.js",
    "import:season": "node src/scripts/import-nfl-season.js",
    "generate:predictions": "node src/scripts/generate-predictions.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.9",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "stripe": "^14.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
